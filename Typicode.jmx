            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.io.File
import java.io.IOException

// Get the working directory based on the environment
String workspaceDir = System.getenv(&quot;WORKSPACE&quot;) ?: System.getProperty(&quot;user.dir&quot;)

// Log the working directory for debugging purposes
log.info(&quot;Working Directory: &quot; + workspaceDir)

// Define the relative path for the CSV file (in the working directory)
String tempCsvFile = workspaceDir + &quot;/output.csv&quot;

// Log the file path that will be used for the CSV file
log.info(&quot;Temp CSV File Path: &quot; + tempCsvFile)

try {
    // Create a new File object for the CSV file
    File tempFile = new File(tempCsvFile)

    // Check if the file exists, and if not, create it
    if (!tempFile.exists()) {
        boolean fileCreated = tempFile.createNewFile()
        if (fileCreated) {
            log.info(&quot;File created successfully: &quot; + tempCsvFile)
        } else {
            log.error(&quot;Failed to create file: &quot; + tempCsvFile)
            return  // Stop the script if file creation fails
        }
    }

    // Get the response data (from JMeter&apos;s SampleResult)
    def responseData = prev.getResponseDataAsString()  // Get the actual response data

    // Append the response data to the CSV file
    tempFile.append(responseData)
    log.info(&quot;Data written to the file: &quot; + tempCsvFile)
} catch (IOException e) {
    log.error(&quot;Error while working with the file: &quot; + e.message)
}
</stringProp>
